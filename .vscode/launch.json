    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Self Hosted",
            "program": "${workspaceFolder}/es6/stepA_mal.mjs",
            "runtimeArgs": [
                "-r",
                "esm"
            ],
            "args": [
                "../mal/stepA_mal.mal"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Repl A",
            "program": "${workspaceFolder}/es6/stepA_mal.mjs",
            "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v12.14.1/bin/node",
            "runtimeArgs": [
                "-r",
                "esm"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Compiler",
            "program": "${workspaceFolder}/es6c/step8_macros.mjs",
            "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v12.14.1/bin/node",
            "runtimeArgs": [
                "-r",
                "esm"
            ],
            "args": ["-c", "tests/predicates.jisp"],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Repl",
            "program": "${workspaceFolder}/es6c/step4_if_fn_do.mjs",
            "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v12.14.1/bin/node",
            "runtimeArgs": [
                "-r",
                "esm"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        }
    ]
}